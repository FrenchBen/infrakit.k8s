{
  "ID": "k8s-controller",
  "Properties": {
    "Allocation": {
      "LogicalIDs": ["172.17.4.101"]
    },
    "Instance": {
      "Plugin": "instance-vagrant",
      "Properties": {
        "Box": "coreos-alpha",
        "BoxVersion": ">= 1151.0.0",
        "BoxURL": "http://alpha.release.core-os.net/amd64-usr/current/coreos_production_vagrant.json",
        "Name": "k8s-controller",
        "File": {
          "Src": "./example/flavor/kubernetes/config/controller-install.sh",
          "Dst": "/tmp/vagrantfile-user-data"
       }
      }
    },
    "Flavor": {
      "Plugin": "flavor-kubernetes",
      "Properties": {
        "Init": [
          "mv /tmp/vagrantfile-user-data /var/lib/coreos-vagrant/"
        ],
        "Role": "controller"
      }
    }
  }
}
